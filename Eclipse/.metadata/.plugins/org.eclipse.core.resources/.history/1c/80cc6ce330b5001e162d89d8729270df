package arrays;

public class array3_last_first {

	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5};
		int n = arr.length;
		int last = arr[arr.length-1];
		int i;
		
		for(i=n-1; i>0; i--) {
			//System.out.println(arr[0]);
			arr[i] = arr[i-1];
			arr[0] = last;	
		}
		System.out.println(java.util.Arrays.toString(arr));
	}
}
